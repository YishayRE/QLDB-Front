---
import DefaultLayout from '../layouts/DefaultLayout.astro'
import DashboardWidget from '../components/DashboardWidget.astro';
import logoSelloVioleta  from "../assets/img/Logo_SelloVioleta.png";
---

<DefaultLayout title="Dashboard">
  <section>
    <h1>Bienvenido</h1><br>
  </section>
  <section class="margin-32">
    <div class="space-content">
      <div class="margin-32" style="border-radius: 10px; background: var(--neutral-200)">
        <img src={logoSelloVioleta} alt="" style="margin: 0 auto; width: 50%;" />
      </div>
    </div>
  </section>
</DefaultLayout>

<script>
  hideLoader();
</script>